parameters:
    level: 5
    paths:
        - src
        - tests
    ignoreErrors:
        -
            message: '~Access to an undefined property NorseBlue\\Prim\\ValueObject::\$nonexistent.~'
            path: %currentWorkingDirectory%/tests/Unit/ValueObjectTest.php
        -
            message: '~Static call to instance method NorseBlue\\Prim\\Tests\\_Helpers_\\Facades\\[a-zA-Z0-9_]+::[a-zA-Z0-9_]+\(\).~'
            path: %currentWorkingDirectory%/tests/Unit/Facade/FacadeTest.php
        -
            message: '~Parameter #1 $value of class NorseBlue\\Prim\\Scalars\\StringObject constructor expects NorseBlue\\Prim\\Scalars\\StringObject|string, int given.~'
            path: %currentWorkingDirectory%/tests/Unit/Scalars/String/StringObjectTest.php
